<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>

		<field name="id"
		       type="text"
		       default="0"
		       label="COM_NENO_FORM_LBL_SOURCE_ID"
		       readonly="true"
		       class="readonly"
		       description="JGLOBAL_FIELD_ID_DESC"/>

		<field name="string"
		       type="textarea"

		       label="COM_NENO_FORM_LBL_SOURCE_STRING"
		       description="COM_NENO_FORM_DESC_SOURCE_STRING"


				/>

		<field name="constant"
		       type="text"

		       label="COM_NENO_FORM_LBL_SOURCE_CONSTANT"
		       description="COM_NENO_FORM_DESC_SOURCE_CONSTANT"


				/>

		<field name="lang"
		       type="language"

		       label="COM_NENO_FORM_LBL_SOURCE_LANG"
		       description="COM_NENO_FORM_DESC_SOURCE_LANG"


				/>

		<field name="extension"
		       type="text"

		       label="COM_NENO_FORM_LBL_SOURCE_EXTENSION"
		       description="COM_NENO_FORM_DESC_SOURCE_EXTENSION"


				/>

		<field name="time_added"
		       type="timecreated"
		       default=""
		       label="COM_NENO_FORM_LBL_SOURCE_TIME_ADDED"
		       description="COM_NENO_FORM_DESC_SOURCE_TIME_ADDED"/>

		<field name="time_changed"
		       type="calendar"
		       class="inputbox"


		       label="COM_NENO_FORM_LBL_SOURCE_TIME_CHANGED"
		       description="COM_NENO_FORM_DESC_SOURCE_TIME_CHANGED"
		       format="%Y-%m-%d %H:%M:%S"
		       filter="safehtml"/>

		<field name="time_deleted"
		       type="calendar"
		       class="inputbox"


		       label="COM_NENO_FORM_LBL_SOURCE_TIME_DELETED"
		       description="COM_NENO_FORM_DESC_SOURCE_TIME_DELETED"
		       format="%Y-%m-%d %H:%M:%S"
		       filter="safehtml"/>

		<field name="version"
		       type="text"
		       size="40"
		       class="inputbox"

		       label="COM_NENO_FORM_LBL_SOURCE_VERSION"
		       description="COM_NENO_FORM_DESC_SOURCE_VERSION"


		       filter="safehtml"/>

	</fieldset>

</form>
