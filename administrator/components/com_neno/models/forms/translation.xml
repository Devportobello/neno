<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>

		<field name="id"
		       type="text"
		       default="0"
		       label="COM_NENO_FORM_LBL_TRANSLATION_ID"
		       readonly="true"
		       class="readonly"
		       description="JGLOBAL_FIELD_ID_DESC"/>

		<field name="source_id"
		       type="text"
		       size="40"
		       class="inputbox"

		       label="COM_NENO_FORM_LBL_TRANSLATION_SOURCE_ID"
		       description="COM_NENO_FORM_DESC_TRANSLATION_SOURCE_ID"


		       filter="safehtml"/>

		<field name="time_translated"
		       type="calendar"
		       class="inputbox"


		       label="COM_NENO_FORM_LBL_TRANSLATION_TIME_TRANSLATED"
		       description="COM_NENO_FORM_DESC_TRANSLATION_TIME_TRANSLATED"
		       format="%Y-%m-%d %H:%M:%S"
		       filter="safehtml"/>

		<field name="version"
		       type="text"
		       size="40"
		       class="inputbox"

		       label="COM_NENO_FORM_LBL_TRANSLATION_VERSION"
		       description="COM_NENO_FORM_DESC_TRANSLATION_VERSION"


		       filter="safehtml"/>

		<field name="lang"
		       type="language"

		       label="COM_NENO_FORM_LBL_TRANSLATION_LANG"
		       description="COM_NENO_FORM_DESC_TRANSLATION_LANG"


				/>

	</fieldset>

</form>
